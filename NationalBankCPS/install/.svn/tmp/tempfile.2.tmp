</script>

<script type="text/javascript">

	function setdelimitersetting(delvalue,divid){
		if(delvalue == "ASCII"){
			document.getElementById(divid).style.display = "block";
		}
		else{
			document.getElementById(divid).style.display = "none";
		}
	}
	
	function showchkno(showval){
		if(showval == "bank"){
			document.getElementById("divchkno").style.display = "none";
			document.getElementById("txtchqnofrom").value = "";
			document.getElementById("txtchqnoto").value = "";
		}
		else if(showval == "chkno"){
			document.getElementById("divchkno").style.display = "block";
		} 
	}
	
		
	function licensctype(gettype){		
		if(gettype == "yearly"){
			document.getElementById("divyearly").style.display = "block"
			document.getElementById("divonetime").style.display = "none"
			document.getElementById("divrevinue").style.display = "none"
		}
		else if(gettype == "onetime"){
			document.getElementById("divyearly").style.display = "none"
			document.getElementById("divonetime").style.display = "block"
			document.getElementById("divrevinue").style.display = "none"
		}
		else if(gettype == "revenue"){
			document.getElementById("divyearly").style.display = "none"
			document.getElementById("divonetime").style.display = "none"
			document.getElementById("divrevinue").style.display = "block"
		}
	}
	
	function calclicenseperiod(){
		var instlndate = document.getElementById("txtdateofinstallyearly").value;
		if(instlndate != ""){
		
			var totaldays = document.getElementById("ddllicenseperiod").value * 365;
			var myDate = new Date(instlndate);
			myDate.setDate(myDate.getDate() + totaldays);
			
			var curr_date = myDate.getDate();
			var curr_month = myDate.getMonth();
			var curr_year = myDate.getFullYear();
			
			document.getElementById("txtlicenseupto").value = curr_month  + "/" + curr_date + "/" + curr_year;
		}
	}
	
	function calclicenseperiodforonetime(){
		//alert("hi");
		var instlndate = document.getElementById("txtdateofinstallonetime").value;
		if(instlndate != ""){
		
			var totaldays = 364;
			var myDate = new Date(instlndate);
			myDate.setDate(myDate.getDate() + totaldays);
			var curr_date = myDate.getDate();
			var curr_month = myDate.getMonth()+1;
			var curr_year = myDate.getFullYear();
						
			document.getElementById("txtlicenseuptoonetime").value = curr_month  + "/" + curr_date + "/" + curr_year;
		}
	}
	
	function getprintermodels(str)
	{
		if (window.XMLHttpRequest)
		{
			xmlhttp=new XMLHttpRequest();
		}
		else
		{
			xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
		}
		xmlhttp.onreadystatechange=function()
		{
			if (xmlhttp.readyState==4 && xmlhttp.status==200)
			{
				
				document.getElementById("divprintermodels").innerHTML="";
				document.getElementById("divprintermodels").innerHTML=xmlhttp.responseText;
			}
		}
		xmlhttp.open("GET","post_installstep4.php?pid="+str,true);
		xmlhttp.send();
	}
	
</script>
